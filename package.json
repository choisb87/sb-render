{
  "name": "n8n-nodes-sb-render",
  "version": "1.3.3",
  "description": "n8n community node for video rendering with customizable subtitles, BGM, and narration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "video",
    "ffmpeg",
    "subtitle",
    "rendering",
    "video-composition"
  ],
  "license": "MIT",
  "homepage": "https://github.com/choisb87/sb-render",
  "author": {
    "name": "choisb87",
    "email": "choisb87@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choisb87/sb-render.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes --ext .ts",
    "lintfix": "eslint nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "postinstall": "node scripts/fix-ffprobe-permissions.js"
  },
  "files": [
    "dist",
    "fonts",
    "scripts"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/SbRender/SbRender.node.js"
    ]
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^18.16.0",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "node-fetch": "^2.6.12",
    "tmp-promise": "^3.0.3"
  },
  "optionalDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
